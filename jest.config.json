{
	"testEnvironment": "jsdom",
	"testPathIgnorePatterns": ["/node_mudules", "/.next"],
	"setupFilesAfterEnv": ["<rootDir>/.jest/setup-tests.js"],
	"moduleNameMapper": {
    "^@src/(.*)": "<rootDir>/src/$1",
    "^@components/(.*)": "<rootDir>/src/components/$1",
    "^@/(.*)": "<rootDir>/src/$1"
  }
}